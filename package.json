{
  "name": "@space-next-door/onslib",
  "version": "1.4.0-local.22",
  "description": "OneSpace Common Library",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "dev": "tsc -w",
    "tsc": "tsc --build",
    "prebuild": "npm run clean",
    "build": "npm run tsc",
    "prepare": "npm run build",
    "lint": "eslint 'src/**/*.{js,ts,jsx,tsx}' --quiet --fix",
    "format": "prettier --ignore-path=.gitignore --loglevel=error --write ./src",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https:git@github.com:space-next-door/onslib.git"
  },
  "author": "OneSpace Team",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^7.4.2",
    "@types/express": "^4.17.11",
    "aws-sdk": "^2.821.0",
    "dayjs": "^1.10.4",
    "express": "^4.17.1",
    "lodash": "^4.17.19",
    "mime-types": "^2.1.28",
    "module-alias": "^2.2.2",
    "node-fetch": "^2.6.1",
    "sharp": "^0.27.0",
    "stream": "^0.0.2",
    "twilio": "^3.62.0",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/mime-types": "^2.1.0",
    "@types/node-fetch": "^2.5.11",
    "@types/qs": "^6.9.7",
    "@types/sharp": "^0.28.4",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.3",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5",
    "semantic-release": "^17.4.5"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "npm run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=14"
  }
}
